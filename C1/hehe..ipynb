    "def Kiemtra(a):\n",
    "    if a < 2:\n",
    "        print(f'{a} khong la SNT')\n",
    "    else:\n",
    "        for i in range(2, a):\n",
    "            if a % i == 0:\n",
    "                print(f'{a} khong la SNT')\n",
    "                break\n",
    "        else:\n",
    "            print(f'{a} la SNT')\n",
    "    return a\n",
    "Kiemtra(2)"
   ]
